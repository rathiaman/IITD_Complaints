<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#FF6A00"
    tools:context="aau.corp.android.app.iitdcomplaints.ParticularPersonalComplaint">


    <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Title"
                android:id="@+id/particular_personal_complaint_title"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginRight="20dp"
                android:textSize="25dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_title_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/particular_personal_complaint_posted_by_answer"
        android:layout_alignStart="@+id/particular_personal_complaint_posted_by_answer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Posted By"
        android:id="@+id/particular_personal_complaint_posted_by"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_title"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_posted_by_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_alignTop="@+id/particular_personal_complaint_posted_by"
        android:layout_toRightOf="@+id/particular_personal_complaint_type"
        android:layout_toEndOf="@+id/particular_personal_complaint_type" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Room No"
        android:id="@+id/particular_personal_complaint_room_no"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_posted_by"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_room_no_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_below="@+id/particular_personal_complaint_posted_by_answer"
        android:layout_alignLeft="@+id/particular_personal_complaint_hostel_answer"
        android:layout_alignStart="@+id/particular_personal_complaint_hostel_answer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Hostel"
        android:id="@+id/particular_personal_complaint_hostel"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_room_no"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_hostel_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_below="@+id/particular_personal_complaint_room_no_answer"
        android:layout_toRightOf="@+id/particular_personal_complaint_type"
        android:layout_toEndOf="@+id/particular_personal_complaint_type" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Contact Info"
        android:id="@+id/particular_personal_complaint_contact_info"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_hostel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_conatct_info_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_below="@+id/particular_personal_complaint_hostel_answer"
        android:layout_alignLeft="@+id/particular_personal_complaint_type_answer"
        android:layout_alignStart="@+id/particular_personal_complaint_type_answer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Complaint type"
        android:id="@+id/particular_personal_complaint_type"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_contact_info"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_type_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_below="@+id/particular_personal_complaint_conatct_info_answer"
        android:layout_toRightOf="@+id/particular_personal_complaint_type"
        android:layout_toEndOf="@+id/particular_personal_complaint_type" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Status"
        android:id="@+id/particular_personal_complaint_status"
        android:textSize="25dp"
        android:layout_below="@+id/particular_personal_complaint_type"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/particular_personal_complaint_status_answer"
        android:text="Answer"
        android:textSize="25dp"
        android:paddingLeft="20dp"
        android:textStyle="italic"
        android:layout_below="@+id/particular_personal_complaint_type_answer"
        android:layout_alignLeft="@+id/particular_personal_complaint_type_answer"
        android:layout_alignStart="@+id/particular_personal_complaint_type_answer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Description"
        android:id="@+id/particular_complaint_description"
        android:layout_below="@id/particular_personal_complaint_status"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="20dp"
        android:layout_alignParentStart="true"
        android:layout_marginRight="20dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Description Answer"
        android:id="@+id/particular_complaint_description_answer"
        android:layout_below="@+id/particular_complaint_description"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="20dp"
        android:layout_marginRight="20dp"
        android:textSize="20dp"
        android:textStyle="italic" />



</RelativeLayout>